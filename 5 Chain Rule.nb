 CellChangeTimes->{{3.70721187331583*^9, 3.7072119091013126`*^9}, {
  3.7072126211510515`*^9, 3.707212676979678*^9}, {3.7072127198999863`*^9, 
  3.70721273409009*^9}, {3.7072169822886167`*^9, 
  3.7072169829270716`*^9}},ExpressionUUID->"956b8135-e82b-4461-902f-\
dc5dd91522ea"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"TableForm", "[", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"level", ",", 
      RowBox[{"Nest", "[", 
       RowBox[{"f", ",", "x", ",", "level"}], "]"}]}], "}"}], ",", 
