  RowBox[{
   RowBox[{"Sin", "[", 
    SuperscriptBox["x", "2.6"], "]"}], ",", "x"}], "]"}]], "Input",
 CellChangeTimes->{{3.7068928505020957`*^9, 3.706892852269354*^9}, {
  3.7068930287584715`*^9, 
  3.706893097233227*^9}},ExpressionUUID->"e76f5af9-8316-4705-b4c8-\
